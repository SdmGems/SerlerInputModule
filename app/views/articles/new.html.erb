<h1>New Article</h1>


<%= form_for :article, url: articles_path do |f| %>
    <p>
    <%= f.label :type %>
    <%= f.text_field :type %><br>
  </p>
  
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %><br>
  </p>
 
  <p>
    <%= f.label :author %>
    <%= f.text_field :author %><br>
  </p>
  
  <p>
    <%= f.label :publisher %>
    <%= f.text_field :publisher %><br>
  </p>

  <p>
    <%= f.label :journal_name %>
    <%= f.text_field :journal_name %><br>
  </p>

  <p>
    <%= f.label :page %>
    <%= f.text_field :page %><br>
  </p>

  <p>
    <%= f.label :year %>
    <%= f.text_field :year %><br>
  </p>
 
  <p>
    <%= f.label :month %>
    <%= f.text_field :month %><br>
  </p>

  <p>
    <%= f.label :DOI %>
    <%= f.text_field :DOI %><br>
  </p>

  <p>
    <%= f.label :volume %>
    <%= f.text_field :volume %><br>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>


<%= render 'form', article: @article %>

<%= link_to 'Back', articles_path %>
